megs: 32
romimage: file=$BXSHARE/BIOS-bochs-latest
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest
vga: extension=vbe
ata0-master: type=disk, path=bochs.img, cylinders=1, heads=4, spt=63
boot: c
log: bochs.log
mouse: enabled=0
cpu: ips=15000000
#vga_update_interval: 150000
magic_break: enabled=1
display_library: x, options="gui_debug"
debug: action=report
info: action=report
error: action=report
panic: action=ask
debug_symbols: file=kernel.ldsym
